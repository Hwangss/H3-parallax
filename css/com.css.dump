/* module 2
 * with a switch
 */
.m-part2 {height: 960px;background: url(../img/page-2-bg.png) center top no-repeat;}
.m-part2 .people{background:  url(../img/page-2-people.png) center top no-repeat;transition:background-position .3s linear;}
.m-part2 .cont{background: url(../img/page-2-cont.png) center top no-repeat;transition:margin-left .3s linear;}

.m-switch,.m-switch figure img{background-image: url(../img/page-2-ui.png);}
.m-switch{position: absolute;top: 291px;left:50%;margin-left:-423px;width: 761px;height: 358px;}
.m-switch figure{position:absolute;padding: 18px 98px 24px 99px;visibility: hidden;}
.m-switch figure.curr{visibility: visible;}
.m-switch figure.curr .view{opacity: 1;}
.m-switch figure .view{width: 563px;height: 319px;-webkit-transition:opacity 1s linear;transition:opacity 1s linear;opacity: 0;}
.m-switch figure .ani{position:absolute;visibility: hidden;}
.m-switch figure .ppl{position:absolute;-webkit-animation:pettry 3s linear infinite;-webkit-transform-origin: center bottom;-moz-animation:pettry 3s linear infinite;-moz-transform-origin: center bottom;animation:pettry 3s linear infinite;transform-origin: center bottom;}
@-webkit-keyframes pettry {
	0%,100%{-webkit-transform: rotate(-5deg);}
	25%,75%{-webkit-transform: rotate(0deg);}
	50%{-webkit-transform: rotate(5deg);}
}
@-moz-keyframes pettry {
	0%,100%{-moz-transform: rotate(-5deg);}
	25%,75%{-moz-transform: rotate(0deg);}
	50%{-moz-transform: rotate(5deg);}
}
@keyframes pettry {
	0%,100%{transform: rotate(-5deg);}
	25%,75%{transform: rotate(0deg);}
	50%{transform: rotate(5deg);}
}

.m-switch .item1 .view{background-position: 0 -400px;}
.m-switch .item1 .ppl{left:-83px;top: 41px;width: 196px;height: 329px;background-position: -800px 0;}
.m-switch .item1 .ani{left:211px;top:-79px;width: 374px;height: 432px;background-position: -599px -750px;}

.m-switch .item2 .view{background-position: 0 -750px;}
.m-switch .item2 .ppl{left:-63px;top: 41px;width: 196px;height: 329px;background-position: -600px -400px;}
.m-switch .item2 .ani{left:212px;top:-139px;width: 420px;height: 478px;background-position: 0 -1450px;}

.m-switch .item3 .view{background-position: 0 -1100px;}
.m-switch .item3 .ppl{left:-79px;top: 61px;width: 177px;height: 303px;background-position: -800px -400px;}
.m-switch .item3 .ani{left:297px;top:-97px;width: 419px;height: 419px;background-position: -500px -1449px;}

.m-switch .trigger{margin-top: 374px;text-align: center;}
