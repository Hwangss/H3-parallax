.m-control{position: relative;height: 263px;}
.m-control li{position:relative;float: left;width: 198px;height: 198px;margin-right: 109px;text-align: center;cursor: pointer;}
.m-control li .icon{position: absolute;left: 0;top:0;width: 198px;height: 198px;}
.m-control li .txt{position:relative;display:inline-block;margin-top:186px;height: 13px;-webkit-transition:margin-top .3s linear;transition:margin-top .3s linear;}
.m-control li .widget{visibility:hidden;position: absolute;top: 170px;height: 56px;opacity: 0;-webkit-transition:all .3s linear;transition:all .3s linear;}
.m-control .item1 .icon{background-position: -200px 0;}
.m-control .item1 .txt{width: 142px;background-position: -650px 0;}
.m-control .item1 .widget{left: 4px;width: 185px;background-position: -438px -100px;}
.m-control .item2 .txt{width: 192px;background-position: -400px 0;}
.m-control .item2 .widget{left: -31px;width: 261px;background-position: -400px -100px;}
.m-control .item3 .icon{background-position: 0 -200px;}
.m-control .item3 .txt{width: 187px;background-position: -400px -50px;}
.m-control .item3 .widget{left: -25px;width: 261px;background-position: -408px -100px;}

.m-control li.curr .icon{left:-80px;top:-80px;width: 358px;height: 358px;-webkit-transform: scale(.554) rotate(0);-moz-transform: scale(.554) rotate(0);-ms-transform: scale(.554) rotate(0);transform: scale(.554) rotate(0);}
.m-control li.curr .txt{margin-top: 230px;height: 15px;}
.m-control li.curr .widget{visibility: visible;opacity: 1;}
.m-control .item1.curr .icon{background-position: 0 -600px;}
.m-control .item1.curr .txt{width: 186px;background-position: -299px -1000px;}
.m-control .item2.curr .icon{background-position: -200px -200px;}
.m-control .item2.curr .txt{width: 261px;background-position: 0 -1000px;margin-left:-31px;}
.m-control .item3.curr .icon{background-position: -400px -600px;}
.m-control .item3.curr .txt{width: 241px;background-position: -500px -1000px;margin-left:-21px;}

